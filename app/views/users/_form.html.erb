
<%#  <%=form_for(@user, url: users_path) do |f|%'> %>
<table class="table table-striped ">
  <%= form_with(model: user, local: true) do |f| %>
    <%=render 'shared/error_messages'%>

    <tbody>
      <tr>
        <td><%= f.label :username%></td>
        <td> <%= f.text_field :username, autofocus: true, placeholder: 'Username' %></td>
      </tr>
      <tr>
        <td><%= f.label :email%></td>
        <td><%= f.email_field :email, placeholder:"example@example.com" %></td>
      </tr>
      <tr>
        <td><%= f.label :password%></td>
        <td><%= f.password_field :password %></td>
        
      </tr>
      <tr>
      <td colspan="1"></td>
        <td id="button"><%= f.submit %></td>
      </tr>
      </tbody>
        

  <%end%>
</table>